{
  "jquery": true,
  "globals": {
    "Morris": false,
    "module": false,
    "require": false,
    "CryptoJS": false,
    "define": false,
    "Apollo": false,
    "SocialCalc": false,
    "__dirname": false,
    "HostManager": false,
    "XLSX": false,
    "XLS": false,
    "L10n": false,
    "FoxDocsAPI": false
  },
  "indent": 2,

  // Gaia use
